<h3>Here are all the tasks in this category:</h3>
<ol>
    @foreach (var task in Model)
    {
        <h4>@task.GetDescription() | <a href="/tasks/@task.GetId()/edit">Edut</a></h4>
    }
</ol>
<p><a href="/categories/@Model["category"].GetId()/tasks/new">Add a new task</a></p>
<p><a href="/">Return to Main Page</a></p>
